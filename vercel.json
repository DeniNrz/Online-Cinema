{
   "version": 2,
   "builds": [
     {
       "src": "next.config.js",
       "use": "@vercel/node"
     },
     {
       "src": "public/**/*",
       "use": "@vercel/static"
     }
   ],
   "routes": [
     {
       "src": "/api/(.*)",
       "dest": "http://localhost:3000"
     },
     {
       "src": "/(.*)",
       "dest": "/"
     }
   ],
   "env": {
     "API_URL": "http://localhost:3000"
   }
 }
 